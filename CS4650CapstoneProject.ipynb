        "        if (re.match(DATE_RE, review_posted_date) and (-1 <= review_score_cleaned <= 1)):\n",
        "            month = review_posted_date.split()[0]\n",
        "            yield month, review_score_cleaned\n",
        "\n",
        "    def reducer(self, key, values):\n",
        "        total = 0.0\n",
